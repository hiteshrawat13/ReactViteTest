{
    "manifest_version": 3,
    "name": "Form to JSON Converter",
    "version": "1.0",
    "description": "Parse all HTML forms on the current page and convert fields into JSON.",
    "permissions": ["scripting", "activeTab"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  